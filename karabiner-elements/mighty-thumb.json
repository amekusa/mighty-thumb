        "type": "basic",
        "from": {
          "key_code": "japanese_eisuu",
          "modifiers": {
            "optional": ["any"]
          }
        },
        "to": [{
          "set_variable": {
            "name": "mightythumb_l",
            "value": 1
          },
          "lazy": true
        }],
        "to_after_key_up": [{
          "set_variable": {
            "name": "mightythumb_l",
            "value": 0
          }
        }],
        "to_if_alone": [{
          "key_code": "japanese_eisuu"
        }]
      }]
    },

